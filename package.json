{
    "name": "rsup-progress",
    "description": "A simple progress bar that supports promise.",
    "version": "1.1.1",
    "repository": "https://github.com/skt-t1-byungi/rsup-progress.git",
    "author": "skt-t1-byungi <tiniwz@gmail.com>",
    "main": "dist/index.umd.js",
    "unpkg": "dist/index.umd.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "keywords": [
        "progress",
        "progressbar",
        "loader",
        "promise"
    ],
    "files": [
        "dist"
    ],
    "license": "MIT",
    "scripts": {
        "dev": "parcel website/index.html -d public",
        "test": "echo skip",
        "build": "rm -rf dist && tsc",
        "umd": "rollup dist/index.js --file dist/index.umd.js --format umd --name RsupProgress",
        "website": "parcel build website/index.html -d public --public-url ./  --no-source-maps",
        "deploy": "rm -rf public && npm run website && gh-pages -d public",
        "prepublishOnly": "npm run test && npm run build && npm run umd"
    },
    "engines": {
        "node": ">= 6"
    },
    "devDependencies": {
        "eslint": "^6.8.0",
        "eslint-config-byungi": "^0.8.1",
        "gh-pages": "^2.2.0",
        "parcel-bundler": "^1.12.4",
        "puppet-run": "^0.10.0",
        "rollup": "^2.1.0",
        "sass": "^1.26.3",
        "thejungle": "^1.0.2",
        "typescript": "^3.8.3"
    },
    "eslintConfig": {
        "extends": "byungi/typescript"
    }
}
